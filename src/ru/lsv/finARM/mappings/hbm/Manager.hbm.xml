<?xml version="1.0" encoding='utf-8'?>

<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ru.lsv.finARM.mappings.Manager" polymorphism="explicit">
        <id name="managerId">
            <generator class="native"/>
        </id>
        <property name="FIO"/>
        <property name="incomeDate"/>
        <property name="subsidy"/>
        <property name="retention"/>
        <property name="cashPercent"/>
        <property name="nonCashPercent"/>
        <property name="dismissed"/>
        <property name="dismissDate"/>
        <property name="salary"/>
    </class>
</hibernate-mapping>
